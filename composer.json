{
    "name": "johndab/oidconnect-laravel",
    "description": "Open ID Connect client library for Laravel Framework",
    "license": "MIT",
    "keywords": ["openidconnect", "oauth", "laravel"],
    "authors": [
        {
            "name": "Artemiy Ryabinkov",
            "email": "getlag@ya.ru"
        }
    ],
    "require": {
        "laravel/socialite": "^5.2",
        "lcobucci/jwt": "^4.1",
        "illuminate/database": "^8.40",
        "illuminate/routing": "^8.40"
    },
    "autoload": {
        "psr-4": {
            "Furdarius\\OIDConnect\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^0.12.93"
    }
}
